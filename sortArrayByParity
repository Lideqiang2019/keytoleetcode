class Solution(object):
    def sortArrayByParity(self, A):
        """
        :type A: List[int]
        :rtype: List[int]
        """
        res=[]
        for i in A:
            if i%2==0:
                res.append(i)
        for i in A:
            if i%2==1:
                res.append(i)
        return res
