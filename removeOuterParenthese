class Solution:
    def removeOuterParentheses(self, S: str) -> str:
        l=0
        res=''
        for i in S:
            if i=='(':#éæœ€å¤–å±‚
                l+=1
                if l>1:
                    res+=i
            else:
                if l>1:
                    res+=i
                l-=1
        return res
