class Solution:
    def isPalindrome(self, x: int) -> bool:
        '''
        将数进行整数翻转，如果翻转后的数和这个整数相同则输出True
        
        '''
        if x>=0:
            a = int(str(x)[::-1])
            if x==a:
                return True
            else:
                return False
        else:
            return False
